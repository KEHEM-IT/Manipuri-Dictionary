{
  "name": "bishnupriya-dictionary",
  "version": "1.0.0",
  "description": "Bishnupriya Dictionary - Combined Frontend and Backend",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server/index.ts",
    "dev:frontend": "vite",
    "build": "npm run build:frontend && npm run build:backend && npm run copy:vercel",
    "build:frontend": "vite build",
    "build:frontend:check": "vue-tsc && vite build",
    "build:backend": "tsc -p tsconfig.server.json && npm run copy-data",
    "copy-data": "node scripts/copy-data.js",
    "copy:vercel": "node scripts/copy-data-vercel.js",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "mysql2": "^3.16.0",
    "vue": "^3.3.11",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.27",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "dictionary",
    "bishnupriya",
    "vue",
    "express"
  ],
  "author": "",
  "license": "ISC"
}
